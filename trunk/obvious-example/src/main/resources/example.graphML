<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
 	 http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
     <key id="d3" for="node" attr.name="nodeId" attr.type="int"/>
     <key id="d0" for="node" attr.name="color" attr.type="string">
       	<default>yellow</default>
	 </key>
	 <key id="d1" for="edge" attr.name="#FirstVertex" attr.type="int"/>
	 <key id="d2" for="edge" attr.name="#SecondVertex" attr.type="int"/>
     <graph id="example" edgedefault="directed">
     	<node id="0">
     		<data key="d0">grey</data>
     		<data key="d3">0</data>
        </node>
     	<node id="1">
     		<data key="d0">red</data>
     		<data key="d3">1</data>
     	</node>
     	<node id="2">
     	    <data key="d3">2</data>
     	</node>
     	<node id="3">
     		<data key="d0">green</data>
     	    <data key="d3">3</data>
     	</node>
     	<node id="4">
     		<data key="d0">pink</data>
     		<data key="d3">4</data>
     	</node>
     	<node id="5">
			<data key="d3">5</data>
     	</node>
     	<edge id="e0" source="0" target="1">
     		<data key="d1">0</data>
     		<data key="d2">1</data>
     	</edge>
     	<edge id="e1" source="0" target="2">
     	    <data key="d1">0</data>
     		<data key="d2">2</data>
     	</edge>
     	<edge id="e2" source="1" target="3">
     	     <data key="d1">1</data>
     		<data key="d2">3</data>
     	</edge>
     	<edge id="e3" source="1" target="4">
     	    <data key="d1">1</data>
     		<data key="d2">4</data>
     	</edge>
     	<edge id="e4" source="3" target="4">
     	    <data key="d1">3</data>
     		<data key="d2">4</data>
     	</edge>
     	<edge id="e5" source="4" target="5">
     	    <data key="d1">4</data>
     		<data key="d2">5</data>
     	</edge> 
     </graph>
</graphml>
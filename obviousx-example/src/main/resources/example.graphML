<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
     http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
     <key id="d0" for="node" attr.name="color" attr.type="string">
       	<default>yellow</default>
	 </key>
	 <key id="d1" for="edge" attr.name="sourceNode" attr.type="int"/>
	 <key id="d2" for="edge" attr.name="targetNode" attr.type="int"/>
     <graph id="example" edgedefault="undirected">
     	<node id="n0">
     		<data key="d0">blue</data>
        </node>
     	<node id="n1">
     		<data key="d0">red</data>
     	</node>
     	<node id="n2"/>
     	<node id="n3">
     		<data key="d0">green</data>
     	</node>
     	<node id="n4">
     		<data key="d0">pink</data>
     	</node>
     	<node id="n5"/>
     	<edge id="e0" source="n0" target="n1">
     		<data key="d1">0</data>
     		<data key="d2">1</data>
     	</edge>
     	<edge id="e1" source="n0" target="n2">
     	    <data key="d1">0</data>
     		<data key="d2">2</data>
     	</edge>
     	<edge id="e2" source="n1" target="n3">
     	     <data key="d1">1</data>
     		<data key="d2">3</data>
     	</edge>
     	<edge id="e3" source="n1" target="n4"/>
     	    <data key="d1">1</data>
     		<data key="d2">4</data>
     	</edge>
     	<edge id="e4" source="n3" target="n4"/>
     	    <data key="d1">3</data>
     		<data key="d2">4</data>
     	</edge>
     	<edge id="e5" source="n4" target="n5"/>
     	    <data key="d1">4</data>
     		<data key="d2">5</data>
     	</edge> 
     </graph>
</graphml>